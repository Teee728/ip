<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<!--> maxHeight="-Infinity" </!-->

<fx:root alignment="TOP_RIGHT" maxWidth="-Infinity" prefHeight="100.0" prefWidth="400.0" style="-fx-background-color: #042654; -fx-background-radius: 15;" type="javafx.scene.layout.HBox" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
    <children>
      <HBox prefHeight="70.0" prefWidth="220.0" style="-fx-background-radius: 15; -fx-background-color: #07387B;">
         <children>
              <Label fx:id="dialog" alignment="TOP_CENTER" minHeight="-Infinity" text="Label" textFill="WHITE" wrapText="true">
                  <font>
                      <Font size="12.0" />
                  </font>
                  <padding>
                      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
                  </padding>
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" right="2.0" top="10.0" />
               </HBox.margin>
              </Label>
         </children>
         <HBox.margin>
            <Insets bottom="10.0" left="10.0" right="20.0" top="10.0" />
         </HBox.margin>
      </HBox>
        <ImageView fx:id="displayPicture" fitHeight="99.0" fitWidth="99.0" pickOnBounds="true" preserveRatio="true" />
    </children>
    <padding>
        <Insets bottom="15.0" left="5.0" right="5.0" top="15.0" />
    </padding>
</fx:root>
