<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.effect.Blend?>
<?import javafx.scene.effect.ColorInput?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<fx:root alignment="TOP_RIGHT" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefWidth="400.0" style="-fx-background-color: #02155a; -fx-background-radius: 20; -fx-border-insets: 20;" type="javafx.scene.layout.HBox" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <Label fx:id="dialog" alignment="TOP_RIGHT" text="Label" textFill="#ffe7a1" wrapText="true">
            <font>
                <Font name="Ebrima Bold" size="14.0" />
            </font>
            <padding>
                <Insets right="30.0" top="20.0" />
            </padding>
            <HBox.margin>
                <Insets right="55.0" top="25.0" />
            </HBox.margin></Label>
        <ImageView fx:id="displayPicture" fitHeight="99.0" fitWidth="99.0" pickOnBounds="true" preserveRatio="true" style="-fx-image: 20;">
            <HBox.margin>
                <Insets />
            </HBox.margin></ImageView>
    </children>
    <padding>
        <Insets bottom="15.0" left="5.0" right="5.0" top="15.0" />
    </padding>
    <effect>
        <Blend>
            <topInput>
                <ColorInput />
            </topInput>
        </Blend>
    </effect>
</fx:root>